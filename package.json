        "react-table": "^7.0.0-rc.16"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.3",
        "@babel/core": "^7.14.3",
        "@babel/preset-env": "^7.14.2",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@types/react-router-dom": "^5.1.7",
        "babel-loader": "^8.2.2",
        "babel-plugin-css-modules-transform": "^1.6.2",
        "prettier": "^2.3.0",
